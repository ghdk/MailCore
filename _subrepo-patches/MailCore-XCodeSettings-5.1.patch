From: Dionysios Kalofonos <peitemou@gmail.com>
Date: Mon, 17 Mar 2014 11:25:06 +0200
Subject: [PATCH] Removes the setting GCC_TREAT_WARNINGS_AS_ERRORS = YES.

---
 MailCore.xcodeproj/project.pbxproj | 2 --
 1 file changed, 2 deletions(-)

diff --git a/MailCore.xcodeproj/project.pbxproj b/MailCore.xcodeproj/project.pbxproj
index deb1578..682533e 100644
--- a/MailCore.xcodeproj/project.pbxproj
+++ b/MailCore.xcodeproj/project.pbxproj
@@ -1037,7 +1037,6 @@
 				DEBUG_INFORMATION_FORMAT = "dwarf-with-dsym";
 				GCC_C_LANGUAGE_STANDARD = "compiler-default";
 				GCC_ENABLE_OBJC_EXCEPTIONS = YES;
-				GCC_TREAT_WARNINGS_AS_ERRORS = YES;
 				GCC_WARN_ABOUT_RETURN_TYPE = YES;
 				GCC_WARN_UNUSED_VARIABLE = YES;
 				HEADER_SEARCH_PATHS = "libetpan/build-mac/include/**";
@@ -1056,7 +1055,6 @@
 				DEBUG_INFORMATION_FORMAT = "dwarf-with-dsym";
 				GCC_C_LANGUAGE_STANDARD = "compiler-default";
 				GCC_ENABLE_OBJC_EXCEPTIONS = YES;
-				GCC_TREAT_WARNINGS_AS_ERRORS = YES;
 				GCC_WARN_ABOUT_RETURN_TYPE = YES;
 				GCC_WARN_UNUSED_VARIABLE = YES;
 				HEADER_SEARCH_PATHS = "libetpan/build-mac/include/**";
